<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhu.authoritymanagement.mapper.CustomerMapper">

    <select id="listCustomer" resultType="com.zhu.authoritymanagement.vo.CustomerVO">
        SELECT customer_id AS id, name, age, sex
        FROM customer
